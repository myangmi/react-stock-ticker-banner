{
	"env": {
			"browser": true,
			"es2021": true,
			"jest/globals": true
	},
	"extends": [
			"eslint:recommended",
			"plugin:react/recommended",
			"airbnb/base"
	],
	"parserOptions": {
			"ecmaFeatures": {
			"jsx": true
			},
			"ecmaVersion": 12,
			"sourceType": "module"
	},
	"plugins": [
			"react",
			"jest"
	],
	"rules": {
			"no-unused-vars": "warn",
			"linebreak-style": "off"
	}
}
